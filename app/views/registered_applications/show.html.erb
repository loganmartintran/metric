<h1><%= @app.name %></h1>
<h4><%= @app.url %></h4>

<div class="col-md-4">
  <%= link_to "Edit", edit_registered_application_path(@app), class: 'btn btn-success' %>
  <%= link_to "Delete", @app, method: :delete, class: 'btn btn-danger', data: { confirm: "Are you sure you want to delete this app?" } %>
</div>
